{
  "name": "vue-app-easy-compta",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env CTX=development node server",
    "build:client": "cross-env CTX=production vite build --outDir dist/prod/client",
    "build:server": "cross-env CTX=production run-p type-check build-only",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build --outDir dist/prod/server --ssr src/server.ts",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false --traceResolution",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/vue": "^2.0.0",
    "@vueuse/core": "^9.5.0",
    "axios": "^1.1.3",
    "bootstrap-icons": "^1.9.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "flag-icons": "^6.6.6",
    "form-data": "^4.0.0",
    "mdb-ui-kit": "^6.0.0",
    "mdb-vue-ui-kit": "^3.1.0",
    "mysql2": "^2.3.3",
    "pinia": "^2.0.23",
    "sequelize": "^6.25.3",
    "uuid": "^9.0.0",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "vue-select": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "cypress": "^10.10.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "terser": "^5.15.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
