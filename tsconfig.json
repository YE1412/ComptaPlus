{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.config.json"
    },
    // {
    //   "path": "./tsconfig.app.json"
    // },
    {
      "path": "./tsconfig.vitest.json"
    }
  ],
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*.tx", "src/**/*.vue", "src/**/*.json"],
  "exclude": ["src/**/__tests__/*", "node_modules/*"],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "downlevelIteration": true,
    "importHelpers": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "paths": {
      "@/*": ["./src/*"],
      "vue": ["node_modules/vue/index.js"],
      "pinia": ["node_modules/pinia/index.js"],
      "vue-i18n": ["node_modules/vue-i18n/index.js"],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "types": [
      "webpack-env",
      "node",
      "json"
    ],
  }
}
